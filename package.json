{
    "name": "web-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start:master": "NODE_ENV=dev vite --open --mode master",
        "start:qa": "NODE_ENV=qa vite --open --mode qa",
        "start:uat": "NODE_ENV=uat vite --open --mode uat",
        "start:prod": "NODE_ENV=prod vite --open --mode prod",
        "preview:master": "NODE_ENV=master vite preview --mode master",
        "preview:qa": "NODE_ENV=qa vite preview --mode qa",
        "preview:uat": "NODE_ENV=uat vite preview --mode uat",
        "preview:prod": "NODE_ENV=prod master vite preview --mode prod",
        "build:master": "NODE_ENV=master vite build --mode master",
        "build:qa": "NODE_ENV=qa vite build --mode qa",
        "build:uat": "NODE_ENV=uat vite build --mode uat",
        "build:prod": "NODE_ENV=prod vite build --mode prod",
        "lint:check": "eslint ./src --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
        "test": "TZ=UTC vitest --run",
        "test:update": "TZ=UTC vitest --run -u",
        "tsc": "tsc",
        "prepare": "husky install"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ]
    },
    "prettier": {
        "arrowParens": "avoid",
        "trailingComma": "es5",
        "tabWidth": 2,
        "semi": false,
        "singleQuote": true
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "eslint --fix ./src --ext .js,.jsx,.ts,.tsx",
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "@auth0/auth0-react": "^2.2.4",
        "@hookform/resolvers": "^3.0.0",
        "@tanstack/react-query": "^5.17.19",
        "@types/react": "^18.0.31",
        "@types/react-dom": "^18.0.11",
        "i18next": "^23.7.18",
        "i18next-browser-languagedetector": "^7.2.0",
        "modern-normalize": "^2.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.49.3",
        "react-hot-toast": "^2.4.0",
        "react-i18next": "^14.0.1",
        "react-modal": "^3.16.1",
        "react-popper": "^2.3.0",
        "react-router-dom": "^6.10.0",
        "react-scroll": "^1.9.0",
        "react-select": "^5.8.0",
        "styled-components": "^6.1.8",
        "vite-plugin-svgr": "^4.2.0",
        "yup": "^1.3.3",
        "zustand": "^4.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.5.0",
        "@commitlint/config-conventional": "^18.5.0",
        "@mobile-reality/eslint-config": "^0.11.0",
        "@tanstack/eslint-plugin-query": "^5.17.20",
        "@testing-library/react": "^14.0.0",
        "@types/node": "^20.11.5",
        "@types/react": "^18.0.31",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "~6.19.1",
        "@typescript-eslint/parser": "~6.19.1",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.37.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "happy-dom": "^13.2.1",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^3.2.4",
        "typescript": "^5.0.3",
        "typescript-plugin-css-modules": "^5.0.0",
        "vite": "^5.0.12",
        "vitest": "^1.2.1"
    }
}
